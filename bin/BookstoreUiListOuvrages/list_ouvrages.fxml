<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="rootPane" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BookstoreUiListOuvrages.list_ouvragesController">
	<children>
		<TableView fx:id="tableView" prefHeight="500.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<columns>
				<TableColumn fx:id="authorCol" prefWidth="75.0" text="Author" />
				<TableColumn fx:id="titleCol" prefWidth="75.0" text="Title" />
				<TableColumn fx:id="priceCol" prefWidth="75.0" text="Price" />
				<TableColumn fx:id="editionCol" prefWidth="75.0" text="Edition" />
				<TableColumn fx:id="isAvailCol" prefWidth="75.0" text="Availability" />
				<TableColumn fx:id="quantiteCol" prefWidth="75.0" text="Quantity" />
				<TableColumn fx:id="descriptionCol" prefWidth="75.0" text="Description" />
			</columns>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
			<contextMenu>
				<ContextMenu>
					<items>
						<MenuItem mnemonicParsing="false" onAction="#handleOuvDeleteOption" text="Delete" />
					</items>
				</ContextMenu>
			</contextMenu>
		</TableView>
	</children>
</AnchorPane>
